<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.app.seenit.seenit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".com.app.seenit.seenit.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".com.app.seenit.seenit.activity.ChapterScreen">
        </activity>
        <activity android:name=".com.app.seenit.seenit.activity.SeasonScreen">
        </activity>
        <activity android:name=".com.app.seenit.seenit.activity.TitleScreen">
        </activity>
        <activity android:name=".com.app.seenit.seenit.activity.AddSerie">
        </activity>

    </application>

</manifest>